AimPrimary(heading, pitch)
{
	signal SIG_AIM;
	set-signal-mask SIG_AIM;
	if (heading > 10920.000000 ) heading = 10920.000000;
	if (heading < -10920.000000 ) heading = -10920.000000;
	if (pitch > 10920.000000 ) pitch = 10920.000000;
	if (pitch < -10920.000000 ) pitch = -10920.000000;
	turn aimy1 to y-axis heading speed 45500.000000;
	turn aimy1 to x-axis 0.000000 - pitch speed 36400.000000;
	wait-for-turn aimy1 around y-axis;

	start-script RestoreAfterDelay();
	return (1);
}

