#define UNITSIZE 10
#define MAXTILT 100












HitByWeapon(anglex, anglez, damage)	
{
	var amount;
	amount = damage / (100 * UNITSIZE);
	var randpiece;
	randpiece = 0;
	if (amount < 3  ) return (0);
	
	if (1){
		randpiece = RAND(1, 3);
		if (randpiece == 1) emit-sfx 1024 from body;
		if (randpiece == 2) emit-sfx 1024 from head;
		if (randpiece == 3) emit-sfx 1024 from tail;
	}
	if (amount > MAXTILT){
		amount = MAXTILT;
	}
	if (anglex ==0 ) { 
		if (anglez == 0){
			amount = amount / 2;
			anglex = RAND(1,500);
			anglez = RAND(1,500);
		}
	}
	
	
	turn BASEPIECE to z-axis (anglez * amount) / 100  speed 4550.000000;
	turn BASEPIECE to x-axis 0.000000 - (anglex * amount) /100 speed 4550.000000;
	wait-for-turn BASEPIECE around z-axis;
	wait-for-turn BASEPIECE around x-axis;
	turn BASEPIECE to z-axis 0.000000 speed 758 ; 
	turn BASEPIECE to x-axis 0.000000 speed 758 ; 
}

static-var isSmoking;
SmokeUnit(healthpercent, randpiece) 
{
	while( TRUE )
	{
		healthpercent = get HEALTH;

		if( healthpercent < 66 )
		{
			randpiece = RAND(1, 3);
			if (randpiece == 1) emit-sfx 1024 from body;
			if (randpiece == 2) emit-sfx 1024 from head;
			if (randpiece == 3) emit-sfx 1024 from tail;
		}
		else {
			isSmoking = 0;
			return;
			
		}
		if (healthpercent < 4) healthpercent = 4;
		sleep healthpercent * 50;
	}
	sleep 97;
	isSmoking = 0;
}

HitByWeaponId(anglex, anglez, weaponid, dmg) 
{
	if( get BUILD_PERCENT_LEFT) return (100);
	if (isSmoking == 0)	{
		isSmoking = 1;
		start-script SmokeUnit();
	}
	
	start-script HitByWeapon(dmg, anglez,anglex); 
	return (100); 
}





